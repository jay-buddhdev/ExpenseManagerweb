<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Project.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrationServlet</servlet-name>
        <servlet-class>Project.RegistrationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>Project.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AssessExpense</servlet-name>
        <servlet-class>Project.AssessExpense</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AssessIncome</servlet-name>
        <servlet-class>Project.AssessIncome</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>viewTransactions</servlet-name>
        <servlet-class>Project.viewTransactions</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExpenseCategoryServlet</servlet-name>
        <servlet-class>Project.ExpenseCategoryServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteExpenseCategory</servlet-name>
        <servlet-class>Project.DeleteExpenseCategory</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IncomeCategoryServlet</servlet-name>
        <servlet-class>Project.IncomeCategoryServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteIncomeCategory</servlet-name>
        <servlet-class>Project.DeleteIncomeCategory</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IncomePieChart</servlet-name>
        <servlet-class>Project.IncomePieChart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExpensePieChart</servlet-name>
        <servlet-class>Project.ExpensePieChart</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrationServlet</servlet-name>
        <url-pattern>/RegistrationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>
            login.html
        </welcome-file>
    </welcome-file-list>
    <servlet-mapping>
        <servlet-name>AssessExpense</servlet-name>
        <url-pattern>/AssessExpense</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AssessIncome</servlet-name>
        <url-pattern>/AssessIncome</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>viewTransactions</servlet-name>
        <url-pattern>/viewTransactions</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExpenseCategoryServlet</servlet-name>
        <url-pattern>/ExpenseCategoryServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteExpenseCategory</servlet-name>
        <url-pattern>/DeleteExpenseCategory</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IncomeCategoryServlet</servlet-name>
        <url-pattern>/IncomeCategoryServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteIncomeCategory</servlet-name>
        <url-pattern>/DeleteIncomeCategory</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IncomePieChart</servlet-name>
        <url-pattern>/IncomePieChart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExpensePieChart</servlet-name>
        <url-pattern>/ExpensePieChart</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
